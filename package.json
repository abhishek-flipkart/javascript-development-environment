{
  "name": "javascript-development-environment",
  "version": "1.0.0",
  "description": "Javascript Development Environment skeleton",
  "scripts": {
		"security-check" : "nsp check",
		"prestart" : "npm run security-check && babel-node buildScripts/startMessage.js",
		"start" : "npm run helper && babel-node buildScripts/srcServer.js",
		"helper" : "echo 'Sample Helper command executing'",
		"posthelper" : "babel-node buildScripts/completeMessage.js",
		"build" : "mkdir dist && babel buildScripts -o dist/index.js && npm run serve",
		"serve" : "nsp check && node dist/index.js",
		"pack":"babel-node buildScripts/srcServer.js"
	},
  "author": "Abhishek Panda",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.26.0",
    "chair": "0.0.0",
    "chalk": "^2.1.0",
    "cheerio": "^1.0.0-rc.2",
    "cross-env": "^5.0.5",
    "eslint": "^4.5.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-watch": "^3.1.2",
    "express": "^4.15.4",
    "html-webpack-plugin": "^2.30.1",
    "jsdom": "^11.2.0",
    "localtunnel": "^1.8.3",
    "mocha": "^3.5.0",
    "nock": "^9.0.14",
    "npm-run-all": "^4.0.2",
    "nsp": "^2.7.0",
    "numeral": "^2.0.6",
    "open": "0.0.5",
    "rimraf": "^2.6.1",
    "webpack": "^3.5.5",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-md5-hash": "0.0.5"
  }
}
